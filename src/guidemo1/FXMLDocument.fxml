<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="guidemo1.FXMLDocumentController">
   <children>
      <HBox prefHeight="220.0" prefWidth="800.0">
         <children>
            <VBox prefHeight="359.0" prefWidth="191.0" spacing="10.0">
               <children>
                  <Label text="CheckBox Objects">
                     <font>
                        <Font name="System Bold Italic" size="18.0" />
                     </font>
                  </Label>
                  <CheckBox fx:id="pepperoniCheckBox" mnemonicParsing="false" text="Pepperoni" />
                  <CheckBox fx:id="beconCheckBox" mnemonicParsing="false" text="Becon" />
                  <CheckBox fx:id="pineAppleCheckBox" mnemonicParsing="false" text="PineApple" />
                  <Button mnemonicParsing="false" onAction="#pizzaOrderButtonPush" text="Order Pizza" />
                  <Label fx:id="label" text="Label" />
               </children>
            </VBox>
            <VBox prefHeight="300.0" prefWidth="200.0" spacing="10.0">
               <children>
                  <Label text="ChoiceBox Object">
                     <font>
                        <Font name="System Bold Italic" size="18.0" />
                     </font>
                  </Label>
                  <ChoiceBox fx:id="choiceBox" prefWidth="150.0" />
                  <Button mnemonicParsing="false" onAction="#choiceBoxButtonPushed" text="Favourite Fruit" />
                  <Label fx:id="choiceBoxLabel" text="Label" />
               </children>
            </VBox>
            <VBox prefHeight="300.0" prefWidth="200.0" spacing="10.0">
               <children>
                  <Label text="ComboBox Object">
                     <font>
                        <Font name="System Bold Italic" size="18.0" />
                     </font>
                  </Label>
                  <ComboBox fx:id="comboBox" prefWidth="150.0" />
                  <Button mnemonicParsing="false" onAction="#comboBoxWasUpdated" text="Choose Course" />
                  <Label fx:id="comboBoxLabel" text="Label" />
               </children>
               <HBox.margin>
                  <Insets />
               </HBox.margin>
               <padding>
                  <Insets left="5.0" />
               </padding>
            </VBox>
            <VBox prefHeight="300.0" prefWidth="200.0" spacing="10.0">
               <children>
                  <Label text="RadioButton Object">
                     <font>
                        <Font name="System Bold Italic" size="18.0" />
                     </font>
                  </Label>
                  <RadioButton fx:id="phpRadioButton" mnemonicParsing="false" text="PHP" />
                  <RadioButton fx:id="javaRadioButton" mnemonicParsing="false" text="JAVA" />
                  <RadioButton fx:id="cSharpRadioButton" mnemonicParsing="false" text="C#" />
                  <RadioButton fx:id="CPlusPlusRadioButton" mnemonicParsing="false" text="C++" />
                  <Button mnemonicParsing="false" onAction="#courseButtonPush" text="Choose Program" />
                  <Label fx:id="radioButtonLabel" text="Label" />
               </children>
            </VBox>
         </children>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </HBox>
      <HBox layoutY="300.0" prefHeight="220.0" prefWidth="800.0" spacing="20.0">
         <children>
            <VBox prefHeight="250.0" prefWidth="200.0">
               <children>
                  <Label text="ListView Object">
                     <font>
                        <Font name="System Bold Italic" size="18.0" />
                     </font>
                  </Label>
                  <ListView fx:id="listView" prefHeight="258.0" prefWidth="198.0" />
               </children>
            </VBox>
            <StackPane prefHeight="238.0" prefWidth="190.0">
               <children>
                  <Button mnemonicParsing="false" onAction="#listViewButtonPushed" prefHeight="25.0" prefWidth="120.0" text="Selected Gear -&gt;" />
               </children>
            </StackPane>
            <VBox prefHeight="250.0" prefWidth="200.0">
               <children>
                  <Label text="TextArea Object">
                     <font>
                        <Font name="System Bold Italic" size="18.0" />
                     </font>
                  </Label>
                  <TextArea fx:id="golfTextArea" prefHeight="220.0" prefWidth="150.0" />
               </children>
            </VBox>
            <AnchorPane prefHeight="200.0" prefWidth="200.0">
               <children>
                  <Button layoutX="117.0" layoutY="161.0" mnemonicParsing="false" onAction="#changeScreenButtonPushed" text="Go to Table View" />
               </children>
            </AnchorPane>
         </children>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </HBox>
   </children>
</AnchorPane>
